    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "@babel/env",
      "@babel/preset-typescript",
      [
        "@babel/preset-react",
        {
          "runtime": "automatic"
        }
      ]
    ],
    "plugins": [
      "@babel/proposal-class-properties",
      "@babel/plugin-transform-runtime",
      [
        "module-resolver",
        {
          "root": [
            "./src"
          ],
          "alias": {
            "@Components": "./components",
            "@Config": "./config",
            "@Routes": "./routes",
            "@Server": "./server",
            "@State": "./state",
            "@Styles": "./styles",
